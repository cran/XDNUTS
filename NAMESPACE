useDynLib(XDNUTS, .registration=TRUE)
importFrom(Rcpp, evalCpp)
importFrom(Rdpack,reprompt)
exportPattern("^[[:alpha:]]+")
S3method(plot,XDNUTS)
export(plot.XDNUTS)
S3method(summary,XDNUTS)
export(summary.XDNUTS)
S3method(print,XDNUTS)
export(print.XDNUTS)
S3method(print,summary.XDNUTS)
export(print.summary.XDNUTS)